def palindrome(str1):
    flag=1
    mid=len(str1)//2
    start=0
    end=len(str1)-1
    while start<=mid:
        if str1[start]==str1[end]:
            start+=1
            end-=1
        else:
            flag=0

    if flag==0:
        print("Given string is not a palindrome")
    else:
        print("Given string is palindrome")
palindrome('madam')        
